{
  "name": "my-app-name",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "publish:main": "yarn clear-react-native-cache && eas update",
    "clear-react-native-cache": "rm -rf $TMPDIR/react-* && rm -rf $TMPDIR/metro-* && rm -rf $TMPDIR/haste-map-*",
    "clear-expo-cache": "rm -rf node_modules && yarn cache clean && yarn && rm -rf $TMPDIR/react-* && rm -rf $TMPDIR/metro-* && rm -rf $TMPDIR/haste-map-*"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.5.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "expo": "^53.0.15",
    "expo-application": "^7.0.7",
    "expo-constants": "~17.1.6",
    "expo-linear-gradient": "^14.0.2",
    "expo-linking": "~7.1.6",
    "expo-router": "~5.1.2",
    "expo-screen-orientation": "^8.1.7",
    "expo-status-bar": "~2.2.3",
    "react": "19.0.0",
    "react-native": "0.79.4",
    "react-native-modal": "^13.0.1",
    "react-native-safe-area-context": "^5.6.1",
    "react-native-screens": "~4.11.1",
    "react-redux": "^9.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0"
  },
  "private": true
}
